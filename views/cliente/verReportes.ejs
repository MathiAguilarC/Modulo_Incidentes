<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <title>Reporte</title>
    <link rel="stylesheet" href="/css/style.css" />
  </head>
  <body>
    <h2>Reportes Enviados</h2>
    <ul>
      <% reportes.forEach(r => { %>
        <li><%= r.titulo %> - <%= r.descripcion %> - <%= r.tipo_error %></li>
      <% }); %>
    </ul>
  </body>
</html>